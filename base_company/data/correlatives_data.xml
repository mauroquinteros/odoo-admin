<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <!-- res.partner.agency Sequence -->
        <record id="agency_sequence" model="ir.sequence">
            <field name="name">Codigo Agencia Secuencia</field>
            <field name="code">res.partner.agency.sequence</field>
