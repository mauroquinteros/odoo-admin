<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="base.main_company" model="res.company">
            <field name="name">JET PERU S.A.</field>
            <field name="favicon" type="base64" file="technical_customizer/static/img/src/favicon.ico"/>
        </record>
        <record id="base.main_partner" model="res.partner" context="{'default_is_company': True}">
            <field name="name">JET PERU S.A.</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="lang">es_PE</field>
            <field name="tz">America/Lima</field>
            <field name="company_type">company</field>
            <field name="website">www.jetperu.com.pe</field>
            <field name="type">enterprise</field>
            <field name="color">2</field>
            <field name="street">Av. Camino Real 395</field>
            <field name='country_id' ref='base.pe'/>
            <field name="state_id" ref="base.state_pe_15"/>
            <field name="image_1920" type="base64" file="technical_customizer/static/img/src/logo.png"/>
            <field name="l10n_latam_identification_type_id" ref="l10n_pe.it_RUC"/>
            <field name="vat">20462526165</field>
            <field name="phone">01 5282819</field>
            <field name="mobile">+511 611 5050</field>
            <field name="email">atencionalcliente@jetperu.com.pe</field>
            <field  name="category_id" eval="[(6, 0, [ref('base_config.res_cat_business_partner')])]"/>
            <field name="customer_rank" eval="0"/>
            <field name="regime_type">general</field>
        </record>
    </data>
</odoo>
